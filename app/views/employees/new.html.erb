<h1>Employees#new</h1>
<p>Find me in app/views/employees/new.html.erb</p>

<% if flash[:message] %>
<%= flash[:message] %>
<%end%>

<%= form_for @employee do |f| %>
<%= f.label :first_name, "First Name" %>
<%= f.text_field :first_name %>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.label :alias %>
<%= f.text_field :alias %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :office %>
<%= f.text_field :office %>
<%= f.label :img_url  %>
<%= f.url_field :img_url %>
<%= f.collection_select(:dog_id, @dogs, :id, :name) %>
<%= f.submit "Submit" %>
<% end %>
